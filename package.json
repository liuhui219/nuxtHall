  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "build": "nuxt build --analyze --dotenv env/.env.production",
    "test": "nuxt build --analyze --dotenv env/.env.test",
    "dev": "nuxt dev --dotenv env/.env.development",
    "generate": "nuxt generate --analyze --dotenv env/.env.production",
